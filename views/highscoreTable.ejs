<% if (users.length == 0) { %>
  <p><%= t('highscore.empty') %></p>
<% } else { %>
  <table class="table table-hover">
    <thead>
      <tr>
        <th scope="col"><%= t('highscore.rank') %></th>
        <th scope="col"><%= t('highscore.name') %></th>
        <th scope="col"><%= t('highscore.score') %></th>
        <th scope="col"><%= t('highscore.lastActive') %></th>
      </tr>
    </thead>
    <tbody>
      <% for (const row of users) { %>
        <tr <%- (user && row.name === user.name) ? `class="table-${App.config.styles.tableHighlightClass}"` : '' %>>
          <td><%=row.rank%></td>
          <td><%=row.name%></td>
          <td><%=row.score%></td>
          <td><%=row.lastActive%></td>
        </tr>
      <% } %>
    </tbody>
  </table>
<% } %> 
